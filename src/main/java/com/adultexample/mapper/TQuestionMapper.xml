  <resultMap type="com.adultexample.manager.dto.QuestionDto" id="pageinfo" extends="BaseResultMap">
  	<result column="subname" property="subject"/>
  	<result column="chaptername" property="chapter"/>
  </resultMap>
  <select id="selectPageInfo" parameterType="string" resultMap="pageinfo">
  	select 
  		question.*,subject.name subname,chapter.name chaptername 
  	from
		(select * from t_question) question
	left join
		(select * from t_subject) subject 
	on 
		question.subject_id = subject.id
	left join 
		(select * from t_chapter) chapter 
	on 
		question.chapter_id = chapter.id 
	where 
		subject.name like '%${value}%' or chapter.name like '%${value}%'
  </select>
  
  
