  <select id="selectPage" parameterType="string" resultMap="page1">
 	select subject1.*,
	case when subject2.name is null then '无上级类型' else subject2.name end parname,userc.createname,
	case when useru.updatename is null then '暂无更新' else useru.updatename end updatename
	from
	(select * from t_subject) subject1
	left join 
	(select * from t_subject)subject2 on subject2.id = subject1.parentno
	left join 
	(select id uidc,realName createname from s_user)userc
	on subject1.create_by = uidc
	left join 
	(select id uidu,realname updatename from s_user)useru
	on subject1.update_by = uidu
	where subject1.name like '%${value}%' or createname like '%${value}%' or updatename like '%${value}%'
	order by sort 
  </select>
  
